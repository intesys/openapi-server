    "@types/body-parser": "^1.17.1",
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.17.2",
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.23",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.17",
    "@types/node-fetch": "^2.5.4",
    "ajv": "^6.10.2",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "commander": "^2.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "jest": "^24.9.0",
    "jest-transform-stub": "^2.0.0",
    "js-yaml": "^3.13.1",
    "json-refs": "^3.0.13",
    "json-schema-resolve-allof": "^1.5.0",
    "lodash": "^4.17.15",
