{
  "name": "openapi-server",
  "version": "4.2.0",
  "description": "A web server based on openApi spec, with proxy, mocks and optional validation",
  "main": "dist/index.js",
  "bin": {
    "openapi-server": "./bin/openapi-server.js"
  },
  "engines": {
    "node": ">9.0.0"
  },
  "scripts": {
    "build": "tsc",
    "changelog": "auto-changelog -u --release-summary",
    "start": "npm run build && node dist/cli.js",
    "prepare": "npm run build",
    "test": "jest",
    "tdd": "jest --watch",
    "coverage": "jest --coverage",
    "install:types": "typesync",
    "example": "npm run build && node dist/cli.js -l --API_YML /examples/api.yml --API_PREFIX /api --STATIC_PATH /examples/static --MOCKS_PATH /examples/mocks"
  },
  "files": [
    "cert/**/*",
    "dist/**/*",
    "bin/**/*",
    "examples/**/*"
  ],
  "author": "alessandro falezza",
  "license": "ISC",
  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.8",
    "@types/js-yaml": "^3.12.5",
    "@types/lodash": "^4.14.158",
    "@types/node": "^14.0.27",
    "@types/node-fetch": "^2.5.7",
    "@types/superagent": "^4.1.8",
    "ajv": "^6.12.3",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "commander": "^4.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "jest": "^26.2.2",
    "jest-transform-stub": "^2.0.0",
    "js-yaml": "^3.14.0",
    "json-refs": "^3.0.15",
    "json-schema-resolve-allof": "^1.5.0",
    "lodash": "^4.17.19",
    "mock-express-request": "^0.2.2",
    "mock-express-response": "^0.2.2",
    "node-fetch": "^2.6.0",
    "openapi-schema-validator": "^7.0.0",
    "openapi-types": "^7.0.0",
    "postman-request": "2.88.1-postman.24",
    "prettier": "^2.0.5",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "ts-jest": "^26.1.4",
    "tslib": "^2.0.0",
    "typescript": "^3.9.7",
    "typesync": "^0.7.0"
  },
  "devDependencies": {
    "@types/prettier": "^2.0.2",
    "@types/request": "^2.48.5",
    "@types/request-promise": "^4.1.46",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "auto-changelog": "^2.2.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4"
  }
}
