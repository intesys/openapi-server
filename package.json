{
  "name": "openapi-server",
  "version": "2.1.0",
  "description": "A web server based on openApi spec, with proxy, mocks and optional validation",
  "main": "dist/index.js",
  "bin": {
    "openapi-server": "./bin/openapi-server.js"
  },
  "engines": {
    "node": ">9.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/cli.js",
