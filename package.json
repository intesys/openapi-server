{
  "name": "openapi-server",
  "version": "4.5.0",
  "description": "A web server based on openApi spec, with proxy, mocks and optional validation",
  "main": "dist/index.js",
  "bin": {
    "openapi-server": "./bin/openapi-server.js"
  },
  "engines": {
    "node": ">9.0.0"
  },
  "scripts": {
    "build": "tsc",
    "changelog": "auto-changelog -u --release-summary",
    "start": "npm run build && node dist/cli.js",
    "prepare": "npm run build",
    "test": "jest",
    "tdd": "jest --watch",
    "coverage": "jest --coverage",
    "install:types": "typesync",
    "example": "npm run build && node dist/cli.js -l --API_YML /examples/api.yml --API_PREFIX /api --STATIC_PATH /examples/static --MOCKS_PATH /examples/mocks",
    "preversion": "npm run build && npm run changelog"
  },
  "files": [
    "cert/**/*",
    "dist/**/*",
    "bin/**/*",
    "examples/**/*"
  ],
  "author": "alessandro falezza",
  "license": "ISC",
  "dependencies": {
    "ajv": "^7.2.1",
    "axios": "^0.21.1",
    "commander": "^7.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "jest-transform-stub": "^2.0.0",
    "js-yaml": "^4.0.0",
    "json-refs": "^3.0.15",
    "json-schema-resolve-allof": "^1.5.0",
    "lodash": "^4.17.21",
    "mock-express-request": "^0.2.2",
    "mock-express-response": "^0.3.0",
    "node-fetch": "^2.6.1",
    "openapi-schema-validator": "^7.2.3",
    "openapi-types": "^7.2.3",
    "postman-request": "^2.88.1-postman.28",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/express-serve-static-core": "^4.17.19",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.21",
    "@types/js-yaml": "^4.0.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.35",
    "@types/node-fetch": "^2.5.8",
    "@types/prettier": "^1.19.1",
    "@types/request": "^2.48.5",
    "@types/request-promise": "^4.1.47",
    "@types/serve-static": "^1.13.9",
    "auto-changelog": "^1.16.4",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "typesync": "^0.8.0"
  }
}