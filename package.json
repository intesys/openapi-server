    "test": "jest",
    "tdd": "jest --watch",
    "coverage": "jest --coverage",
    "install:types": "types-installer install"
  },
  "files": [
    "dist/**/*",
    "bin/**/*",
    ".env",
    "api.yml"
  ],
  "author": "alessandro falezza",
  "license": "ISC",
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.11",
    "@types/js-yaml": "^3.12.0",
    "@types/lodash": "^4.14.123",
    "@types/node": "^11.11.3",
    "ajv": "^6.10.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "commander": "^2.19.0",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "jest": "^24.5.0",
    "jest-transform-stub": "^2.0.0",
    "js-yaml": "^3.12.2",
    "json-refs": "^3.0.12",
    "json-schema-resolve-allof": "^1.5.0",
    "lodash": "^4.17.11",
    "mock-express-request": "^0.2.2",
