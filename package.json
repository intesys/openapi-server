    "prepare": "npm run build",
    "test": "jest",
    "tdd": "jest --watch",
    "coverage": "jest --coverage",
    "install:types": "typesync"
  },
  "files": [
    "cert/**/*",
    "dist/**/*",
    "bin/**/*",
    "examples/**/*"
  ],
  "author": "alessandro falezza",
  "license": "ISC",
